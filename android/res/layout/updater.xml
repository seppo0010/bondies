<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="wrap_content">
	<TextView android:text="@+string/defaultUrlLabel"
		android:layout_width="fill_parent" android:layout_height="wrap_content"></TextView>
	<EditText android:text="@+string/defaultUrl" android:id="@+id/url"
		android:layout_width="fill_parent" android:layout_height="wrap_content"></EditText>
	<Button android:text="@+string/checkVersion" android:id="@+id/checkVersion"
		android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
</LinearLayout>